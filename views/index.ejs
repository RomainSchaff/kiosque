<!DOCTYPE html>
<html lang="en">
<head>
      <meta charset="UTF-8">
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <title>KIOSQUE V2</title>

      <link rel="icon" type="image/png" href="/assets/icons/favicon.png" />


      <link rel="stylesheet" href="/css/style.css">
      <link rel="stylesheet" href="/css/responsive.css">

</head>
<body>


      <header>
            <div id="boxsignal_container">
                  <p id="box_name">Box4G</p>
                  <div id="boxsignal_barbox">
                        <div></div>
                        <div></div>
                        <div></div>
                  </div>
            </div>
            <div id="date_time_container">
                  <p>-</p>
            </div>
      </header>
      <main>
            <section id="main_header_container">
                  <div id="searchbar_container">
                        <img src="/assets/search_icon.svg" alt="">
                        <input id="searchbar_input" type="text" placeholder="rechercher">
                  </div>
                  <div id="notification_box" onclick="openNotifContainer()">
                        <img src="/assets/notif_icon.svg" alt="">
                        <div id="notifbox_number">
                              <p>0</p>
                        </div>
                  </div>
            </section>
            <section id="main_content_container">
                  <div id="maincontent_category_btn_container">
                        <div id="maincontent_sessionbox" onclick="handleChangeCategory(event, 'specific')">
                              <div id="maincontent_sessionbox_background"></div>
                              <div id="maincontent_sessionbox_infos">
                                    <p>session</p>
                                    <p id="date_session">-</p>
                                    <img id="change_daytosession_btn" src="/assets/arrow_fill_icon.svg" onclick="handleChangeSessionDay()" alt="">
                              </div>
                              <img class="arrow_btn" onclick="handleChangeNextPrevDay('prev')" src="/assets/chevron_left_icon.svg">
                              <img class="arrow_btn" onclick="handleChangeNextPrevDay('next')" src="/assets/chevron_right_icon.svg">
                        </div>
                        <div id="maincontent_genericbox" onclick="handleChangeCategory(event, 'generic')">
                              <div id="maincontent_genericbox_background"></div>
                              <p>generiques</p>
                        </div>
                  </div>
                  <div id="maincontent_allresult_container">

                        <div id="maincontent_allresult_innercontainer"></div>

                        <!-- Interface box model Specific -->
                        <!-- <div class="interface_container">
                              <div class="interface_contentbox">
                                    <div class="interface_contentbox_topinfosbox">
                                          <p class="interface_category">spécifique</p>
                                          <p class="interface_date_update">05/09/2023 - 16h02</p>
                                    </div>
                                    <div class="interface_contentbox_maininfosbox">
                                          <p class="interface_numseq">1921.3</p>
                                          <p class="interface_description">Tel Karim appel entrant Lambda</p>
                                    </div>
                              </div>
                              <div class="interface_remote_iconbox">
                                    <img src="/assets/remote_icon.svg" alt="">
                              </div>
                        </div> -->

                  </div>
            </section>
      </main>


      <!-- NOTIF LAYER CONTAINER -->
      <section id="notif_container">
            <div id="notif_innercontainer">
                  <p id="notif_innercontainer_title">notifications</p>
                  <img id="notif_innercontainer_closeicon" onclick="closeNotifContainer()" src="/assets/close_icon.svg" alt="">
                  <div id="all_notifbox_container">

                        <!-- MODEL NOTIFICATIONS -->
                        <!-- <div class="notif_box">
                              <p class="notif_date">06/09/2023 à 00h12</p>
                              <p class="notif_description">box non connectée depuis le 06/09/2023 à 01h23</p>
                        </div> -->

                  </div>
            </div>
      </section>
      


      <script src="/socket.io/socket.io.js"></script>
      <script src="/js/script.js"></script>
</body>
</html>